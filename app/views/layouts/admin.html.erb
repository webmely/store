<head>
  <title><%= content_for?(:title) ? content_for(:title) : "Demo website" %> | Demo website description</title>

    <!-- mobile settings -->
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700&subset=latin,vietnamese" rel="stylesheet" type="text/css" />

  <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'admin', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper" class="clearfix">
    <%= render "admin/shared/header" %>
    <section id="middle">
      <%= yield %>
    </section>
    <script type="text/javascript">var plugin_path = 'assets/plugins/';</script>
  </div>
</body>
</html>